{
  "name": "stb",
  "version": "1.0.0",
  "description": "storyboards portfolio built with Jekyll and Tachyons",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/YJPL/film-storyboards/",
    "scripts": {
      "gulp": "gulp"
    }
  },
  "keywords": [
    "portfolio",
    "storyboards"
  ],
  "author": "YJPL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YJPL/film-storyboards/issues"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-size": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "immutable-css-cli": "^1.1.1",
    "watch": "^1.0.2"
  },
  "contributors": [
    {
      "name": "YJPL",
      "email": ""
    }
  ],
  "scripts": {
    "start": "npm run build:watch",
    "mutations": "immutable-css src/tachyons.css --strict",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tachyons src/tachyons.css > css/tachyons.css",
    "build:minify": "tachyons src/stb-tachyons.css -m > css/stb-tachyons.min.css",
    "build:watch": "watch 'npm run build' ./src/"
  },
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "directory-tree": "^2.2.4",
    "gulp-basswork": "^0.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^2.0.0"
  }
}
