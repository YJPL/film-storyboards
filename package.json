{
  "name": "FilmStoryboards",
  "version": "1.0.0",
  "description": "storyboards portfolio built with Jekyll and Tachyons",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/YJPL/film-storyboards/",
    "scripts": {
      "gulp": "gulp"
    }
  },
  "keywords": [
    "portfolio",
    "storyboards"
  ],
  "author": "YJPL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YJPL/film-storyboards/issues"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.13",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-size": "^4.0.0",
    "immutable-css-cli": "^1.1.1",
    "postcss": "^8.4.14",
    "watch": "^1.0.2"
  },
  "contributors": [
    {
      "name": "YJPL",
      "email": ""
    }
  ],
  "scripts": {
    "start": "npm run build:watch",
    "mutations": "immutable-css src/stb-tachyons.css --strict",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tachyons src/stb-tachyons.css > css/stb-tachyons.css",
    "build:minify": "tachyons src/stb-tachyons.css --minify > css/stb-tachyons.min.css",
    "build:watch": "watch \"npm run build\" ./src/"
  },
  "dependencies": {
    "browser-sync": "^2.27.10",
    "del": "^7.0.0",
    "glob": "^8.0.3",
    "gulp-basswork": "^0.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "sharp": "^0.31.0"
  }
}
